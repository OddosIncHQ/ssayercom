let __exports = {};
/** @odoo-module */

const { ProductScreen } = require("@point_of_sale/../tests/tours/helpers/ProductScreenTourMethods");
const { ReceiptScreen } = require("@point_of_sale/../tests/tours/helpers/ReceiptScreenTourMethods");
