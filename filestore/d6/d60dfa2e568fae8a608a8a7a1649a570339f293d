                run: () => {},
            },
        ];
    }
    pressNumpad(key) {
        let trigger;
        if (".0123456789".includes(key)) {
            trigger = `.numpad .number-char:contains("${key}")`;
        } else if (key === "Backspace") {
            trigger = `.numpad .numpad-backspace`;
        } else if (key === "+/-") {
            trigger = `.numpad .numpad-minus`;
        }
        return [
            {
                trigger,
            },
        ];
    }
    confirmRefund() {
        return [
            {
                trigger: ".ticket-screen .button.pay-order-button",
            },
        ];
    }
}

