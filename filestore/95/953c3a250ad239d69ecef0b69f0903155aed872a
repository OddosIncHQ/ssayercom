deleteRange(sel){if(this.deleteTableRange()){return;}
if(!this.editable.childElementCount){const p=document.createElement('p');p.append(document.createElement('br'));this.editable.append(p);setSelection(p,0);return;}
let range=getDeepRange(this.editable,{sel,splitText:true,select:true,correctTripleClick:true,});if(!range)return;const commonAncestorContainer=this.editable.contains(range.commonAncestorContainer)?range.commonAncestorContainer:this.editable;const startUneditable=getFurthestUneditableParent(range.startContainer,commonAncestorContainer);if(startUneditable){let leaf=previousLeaf(startUneditable);if(leaf){range.setStart(leaf,nodeSize(leaf));}else{range.setStart(commonAncestorContainer,0);}}
