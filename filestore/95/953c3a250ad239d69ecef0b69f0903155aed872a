historyGetMissingSteps({fromStepId,toStepId}){const fromIndex=this._historySteps.findIndex(x=>x.id===fromStepId);const toIndex=this._historySteps.findIndex(x=>x.id===toStepId);if(fromIndex===-1||toIndex===-1){return-1;}
