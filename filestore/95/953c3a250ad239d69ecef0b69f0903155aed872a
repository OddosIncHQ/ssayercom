_multiselectionDisplayClient({selection,color,clientId,clientAvatarUrl='',clientName=this.options._t('Anonymous')}){let clientRects;let anchorNode=this.idFind(selection.anchorNodeOid);let focusNode=this.idFind(selection.focusNodeOid);let anchorOffset=selection.anchorOffset;let focusOffset=selection.focusOffset;if(!anchorNode||!focusNode){anchorNode=this.editable.children[0];focusNode=this.editable.children[0];anchorOffset=0;focusOffset=0;}
