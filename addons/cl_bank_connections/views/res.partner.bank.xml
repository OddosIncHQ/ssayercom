<odoo>
  <data>
    <record id="res_partner_bank_form_fintoc_token" model="ir.ui.view">
      <field name="name">res.partner.bank.form.fintoc.token</field>
      <field name="model">res.partner.bank</field>
      <field name="inherit_id" ref="base.view_partner_bank_form"/>
      <field name="mode">extension</field>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='partner_id']" position="after">
          <field name="fintoc_token" string="Fintoc Token" password="True"/>
        </xpath>
      </field>
    </record>

  </data>
</odoo>