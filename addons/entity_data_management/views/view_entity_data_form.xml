<odoo>
    <record id="view_entity_data_form" model="ir.ui.view">
        <field name="name">entity.data.form</field>
        <field name="model">entity.data</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="vat"/>
                        <field name="type"/>
                    </group>
                    <group>
                        <field name="date_time"/>
                        <field name="upload_datetime"/>
                        <field name="upload_type"/>
                    </group>
                    <group>
                        <field name="file_code"/>
                        <field name="file_attachment"/>
                        <field name="file_processed"/>
                        <field name="pdf_form_result"/>
                    </group>
                    <group string="Email CSV Data">
                        <field name="email_date"/>
                        <field name="csv_file_name"/>
                        <field name="email_sender"/>
                        <field name="rut"/>
                        <field name="csv_creation_date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
