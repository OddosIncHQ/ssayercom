<odoo>
    <record id="view_form_entity_data_custom" model="ir.ui.view">
        <field name="name">entity.data.form.custom</field>
        <field name="model">entity.data</field>
        <field name="arch" type="xml">
            <form string="Entity Data">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="type"/>
                            <field name="file_code"/>
                            <field name="file_processed"/>
                            <field name="upload_datetime"/>
                        </group>
                        <group>
                            <field name="vat"/>
                            <field name="date_time"/>
                            <field name="file_attachment"/>
                            <field name="pdf_form_result"/>
                            <field name="upload_type"/>
                        </group>
                    </group>
                    <group>
                        <separator/>
                    </group>
                    <footer>
                        <button name="process_file_button" type="object" string="Process File" class="btn-primary"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
