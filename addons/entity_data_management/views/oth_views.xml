<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_oth_entity_data_form" model="ir.ui.view">
        <field name="name">oth.entity.data.form</field>
        <field name="model">oth.entity.data</field>
        <field name="arch" type="xml">
            <form string="Other Entity Data">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="vat"/>
                        <field name="type"/>
                        <field name="date_time"/>
                        <field name="file_code"/>
                        <field name="file_attachment"/>
                        <field name="file_processed"/>
                        <field name="pdf_form_result"/>
                        <field name="upload_datetime"/>
                        <field name="upload_type"/>
                    </group>
                    <group>
                        <!-- Dynamic fields will be added here -->
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_oth_entity_data_tree" model="ir.ui.view">
        <field name="name">oth.entity.data.tree</field>
        <field name="model">oth.entity.data</field>
        <field name="arch" type="xml">
            <tree string="Other Entity Data">
                <field name="name"/>
                <field name="vat"/>
                <field name="type"/>
                <field name="date_time"/>
                <field name="file_code"/>
                <field name="upload_datetime"/>
                <field name="upload_type"/>
            </tree>
        </field>
    </record>
</odoo>
