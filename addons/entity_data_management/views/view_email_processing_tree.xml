<odoo>
    <record id="view_email_processing_tree" model="ir.ui.view">
        <field name="name">email.processing.tree</field>
        <field name="model">custom_fetchmail_server</field> <!-- or 'email.processing' depending on your context -->
        <field name="arch" type="xml">
            <tree string="Email Processing">
                <field name="name"/>
                <field name="sender"/>
                <field name="received_date"/>
                <!-- Add other necessary fields -->
            </tree>
        </field>
    </record>
</odoo>
