<odoo>
    <record id="view_contact_form_automation_form" model="ir.ui.view">
        <field name="name">contact.form.automation.form</field>
        <field name="model">contact.form.automation</field>
        <field name="arch" type="xml">
            <form string="Contact Form Automation">
                <sheet>
                    <group>
                        <field name="rut_cl"/>
                        <field name="sii_pass"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <menuitem id="contact_form_automation_menu" name="Contact Form Automation" sequence="10"/>

    <menuitem id="contact_form_automation_sub_menu" name="Contact Form" parent="contact_form_automation_menu" action="action_contact_form_automation"/>

    <record id="action_contact_form_automation" model="ir.actions.act_window">
        <field name="name">Contact Form</field>
        <field name="res_model">contact.form.automation</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
