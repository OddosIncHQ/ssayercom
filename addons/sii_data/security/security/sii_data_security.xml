<odoo>
    <data noupdate="1">
        <!-- Group: SII Data User -->
        <record id="group_sii_data_user" model="res.groups">
            <field name="name">SII Data User</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>

        <!-- Group: SII Data Manager -->
        <record id="group_sii_data_manager" model="res.groups">
            <field name="name">SII Data Manager</field>
            <field name="implied_ids" eval="[(4, ref('group_sii_data_user'))]"/>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>
    </data>
</odoo>
